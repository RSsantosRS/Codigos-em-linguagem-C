#include<stdlib.h> 
#include<stdio.h> 
    int main(void){ 
        int n,divi,ndivi; 
        divi=ndivi=0; 
        printf("bem vindo ao programa verificador de divisiveis\n"); 
      	for(int i=0;i<10;i++){ 
        	    printf("digite o %d° valor\n",i+1); 
          	    scanf("%d",&n); 
            if (n %3==0 && n%5==0 ){ 
                divi++;                                                 
            }else{ 
               ndivi++; 
            } 
        } 
       	 printf("A quantidade de valores divisiveis por 3 e 5 ao mesmo tempo foi de %d\n",divi); 
       	 printf("A quantidade de valores não divisiveis foi de %d\n",ndivi); 
       return(0); 
    } 
